        {
          "path": "divinatory-symbol/Detail",
          "style": {
            "navigationBarTitleText": "查看卦象"
          },
          "activeTabPath": "pages/index/index"
        }
      ]
    }
  ],
  "globalStyle": {
    "navigationStyle": "custom",
    "mp-weixin": {}
  }
}